datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator erd {
    provider = "node ./dist/index.js"
    output   = "../../__tests__/212.svg"
    erdDebug = true
}

model User {
    id    Int     @id @default(autoincrement())
    email String  @unique
    name  String?
    Test  String? @map("2.1.2")
}

